<Window x:Class="MistralChat.ProfilerOutputWindow"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		Title="GPU Profiler Data"
		Width="800" Height="320"
		MinWidth="800" MinHeight="370"
		PreviewKeyDown="window_PreviewKeyDown">
	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition Height="Auto" />
			<RowDefinition />
		</Grid.RowDefinitions>
		<TextBlock Text="Compute shaders" HorizontalAlignment="Center" FontFamily="Consolas" FontSize="16" />
		<TextBox Grid.Row="1" IsReadOnly="True" TextWrapping="Wrap" x:Name="tbShaders"
			FontFamily="Consolas" FontSize="13.333" Padding="4" />
	</Grid>
</Window>